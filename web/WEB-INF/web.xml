<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Core Skills Test</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <description/>
        <display-name>QuizReceiverController</display-name>
        <servlet-name>QuizReceiverController</servlet-name>
        <servlet-class>uk.ac.angus.coreskillstest.controller.QuizReceiverController</servlet-class>
    </servlet>
        <servlet-mapping>
        <servlet-name>QuizReceiverController</servlet-name>
        <url-pattern>/Quiz/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>QuizEventController</servlet-name>
        <servlet-class>uk.ac.angus.coreskillstest.controller.QuizEventController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QuizEventController</servlet-name>
        <url-pattern>/Event/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <description/>
        <display-name>UserReceiverController</display-name>
        <servlet-name>UserReceiverController</servlet-name>
        <servlet-class>uk.ac.angus.coreskillstest.controller.UserReceiverController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserReceiverController</servlet-name>
        <url-pattern>/User/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <description/>
        <display-name>ResultReceiverController</display-name>
        <servlet-name>ResultReceiverController</servlet-name>
        <servlet-class>uk.ac.angus.coreskillstest.controller.ResultReceiverController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResultReceiverController</servlet-name>
        <url-pattern> /Result/*</url-pattern>
    </servlet-mapping>
</web-app>
